<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<item>
    <shape android:shape="rectangle">
        <corners android:radius="180dp" />
        <solid android:color="@color/white" />
    </shape>
</item>
<item
    android:bottom="2dp"
    android:left="2dp"
    android:right="2dp"
    android:top="2dp">
    <shape android:shape="rectangle">
        <corners android:radius="180dp" />
        <gradient
            android:startColor="@color/settingBackground2"
            android:endColor="@color/settingBackground1"
            android:angle="90"/>
    </shape>
</item>


</layer-list>